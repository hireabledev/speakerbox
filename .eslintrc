{
  "parserOptions": {
    "ecmaVersion": "2017",
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "mocha": true
  },
  "globals": {
    "ga": false,
    "mixpanel": false,
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "rules": {
    "no-console": [2, { "allow": ["info", "warn", "error"] }],
    "no-unused-vars": [2, { "argsIgnorePattern": "next|reject" }],
    "arrow-parens": 0,
    "prefer-template": 1,
    "import/prefer-default-export": 0,
    "import/no-named-as-default": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-shadow": ["error", { "allow": ["res"] }],
    "react/no-multi-comp": 1,
    "react/forbid-prop-types": 0,
    "react/no-unused-prop-types": [1, { skipShapeProps: true }]
  }
}
