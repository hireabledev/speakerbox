{
  "presets": [
    "es2015",
    "react",
    "stage-3"
  ],
  "plugins": [
    "transform-runtime",
    ["module-resolver", {
      "root": ["./src"]
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        ["istanbul", {
          "exclude": ["**/*.spec.js"]
        }]
      ]
    }
  }
}
